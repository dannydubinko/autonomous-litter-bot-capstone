<?xml version="1.0"?>
<robot name="go1_robot">

  <material name="blue">
      <color rgba="0.0 0.0 1.0 1.0"/>
  </material>

  <material name="pink">
      <color rgba="1.0 0.75 0.79 1.0"/>
  </material>

  <material name="silver">
      <color rgba="0.75 0.75 0.75 1.0"/>
  </material>

  <link name="base_footprint"/>

  <joint name="base_footprint_joint" type="fixed">
      <parent link="base_footprint"/>
      <child link="base_link"/>
      <origin xyz="0 0 0.3" rpy="0 0 0"/> </joint>

  <link name="base_link">
      <visual>
          <geometry>
              <box size="0.4 0.2 0.1"/>
          </geometry>
          <origin rpy="0 0 0" xyz="0 0 0"/>
          <material name="silver"/>
      </visual>
  </link>

  <joint name="lidar_joint" type="fixed">
      <parent link="base_link"/>
      <child link="laser"/> <origin xyz="0.18 0 -0.075" rpy="3.14159 0 3.14159"/>
  </joint>

  <link name="laser">
      <visual>
          <geometry>
              <cylinder length="0.05" radius="0.04"/>
          </geometry>
          <origin rpy="0 0 0" xyz="0 0 0"/> 
          <material name="pink"/>
      </visual>
  </link>

  <joint name="imu_joint" type="fixed">
      <parent link="base_link"/> <child link="imu_link"/>   
      <origin xyz="0.18 0.05 -0.075" rpy="0 0 0"/>
  </joint>

  <link name="imu_link">
      <visual>
          <geometry>
              <box size="0.015 0.02 0.005"/>
          </geometry>
          <origin rpy="0 0 0" xyz="0 0 0"/>
          <material name="blue"/>
      </visual>
  </link>

</robot>